@model IEnumerable<CarShop.Models.User>
<h1>User list</h1>

<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a asp-action="Edit" asp-route-userid="@user.Id" class="btn btn-primary">Change roles</a>
            </td>
        </tr>
    }
</table>